{
	"name": "qslwidgets",
	"version": "0.2.44",
	"scripts": {
		"build:browser": "rollup -c rollup.config.js src/browser.ts",
		"build:eel": "rollup -c rollup.config.js src/eel.ts && cp src/eel.html dist/eel/index.html",
		"build:anywidget": "rollup -c rollup.config.js src/anywidget.ts",
		"build:svelte": "svelte-package -i src -o dist/svelte",
		"build": "run-s build:*"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"svelte": "./dist/svelte/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@anywidget/types": "^0.2.0",
		"@rollup/plugin-commonjs": "^28.0.6",
		"@rollup/plugin-node-resolve": "^16.0.0",
		"@rollup/plugin-replace": "^6.0.2",
		"@rollup/plugin-typescript": "^12.0.0",
		"@sveltejs/package": "^2.0.0",
		"@tsconfig/svelte": "^5.0.0",
		"@types/file-saver": "^2.0.7",
		"@types/rollup-plugin-css-only": "^3.1.3",
		"@types/uuid": "^10.0.0",
		"@use-gesture/vanilla": "^10.0.0",
		"@wasm-tool/rollup-plugin-rust": "^3.0.5",
		"binaryen": "^121.0.0",
		"file-saver": "^2.0.0",
		"html2canvas": "^1.0.0",
		"nouislider": "^15.0.0",
		"npm-run-all": "^4.1.5",
		"rollup": "^4.0.0",
		"rollup-plugin-css-only": "^4.0.0",
		"rollup-plugin-svelte": "^7.0.0",
		"svelte": "^4.0.0",
		"svelte-preprocess": "^6.0.0",
		"typescript": "^5.0.0",
		"uuid": "^7.0.0"
	},
	"keywords": [
		"svelte"
	]
}